{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn prisma:generate && npx rimraf .next && next dev",
    "build": "yarn prisma:generate && npx rimraf .next && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "npx prisma migrate dev --schema ./prisma/scheme.prisma && prisma generate --schema ./prisma/scheme.prisma",
    "seed": "yarn prisma:generate && node ./prisma/seed.js",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@logtail/js": "^0.5.2",
    "@prisma/client": "5.20.0",
    "@svgr/webpack": "^8.1.0",
    "@types/logger": "^0.0.5",
    "logger": "^0.0.1",
    "next": "15.2.1",
    "notistack": "^3.0.2",
    "patch-package": "^8.0.0",
    "polished": "^4.3.1",
    "prisma": "5.20.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "typescript": "^5"
  }
}
